{
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": "src",
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "paths": {
      "apexcharts": [
        "../node_modules/apexcharts-clevision"
      ],
      "apexcharts/*": [
        "../node_modules/apexcharts-clevision/*"
      ]
    },
    "incremental": false
  },
  "include": [
    "src"
  ],
  "exclude": [
    "../node_modules",
    "../app"
  ]
}
